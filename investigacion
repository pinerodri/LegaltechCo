     <script src="https://legalzoomcdn.net/static/onetrust.js"></script>
  <script>
    !function(){var analytics=window.analytics=window.analytics||[];if(!analytics.initialize)if(analytics.invoked)window.console&&console.error&&console.error("Segment snippet included twice.");else{analytics.invoked=!0;analytics.methods=["trackSubmit","trackClick","trackLink","trackForm","pageview","identify","reset","group","track","ready","alias","debug","page","once","off","on","addSourceMiddleware","addIntegrationMiddleware","setAnonymousId","addDestinationMiddleware", "register"];analytics.factory=function(e){return function(){if(window.analytics.initialized)return window.analytics[e].apply(window.analytics,arguments);var i=Array.prototype.slice.call(arguments);i.unshift(e);analytics.push(i);return analytics}};for(var i=0;i<analytics.methods.length;i++){var key=analytics.methods[i];analytics[key]=analytics.factory(key)}analytics.load=function(key,i){var t=document.createElement("script");t.type="text/javascript";t.async=!0;t.src="https://proxy.legalzoom.com/segments-cdn/analytics.js/v1/" + key + "/analytics.min.js";var n=document.getElementsByTagName("script")[0];n.parentNode.insertBefore(t,n);analytics._loadOptions=i};analytics._writeKey="TiERpYXvvRVle5SyXPSNJssBISaF5Yyd";;analytics.SNIPPET_VERSION="4.16.1";

          function findPageSection(element) {
            if(!element){
                return;
            }
            if (element.closest('nav')) {
                return 'left nav';
            }
            if (element.querySelector('header')) {
                return element.querySelector('header').innerText;
            }
            if (element.classList.contains('nav-container')) {
                return 'global nav';
            }
            if (element.querySelector('h3')) {
                return element.querySelector('h3').innerText;
            }
            if (element.querySelector('h2')) {
                return element.querySelector('h2').innerText;
            }
            if (element.querySelector('h1')) {
                return element.querySelector('h1').innerText;
            }
            const pageSection = findPageSection(element.parentElement);
            if (pageSection) {
              return pageSection.replace(/[\r\n ]+/g,' ');
            }
          }

          function isTaxURL() {
              return window.location.pathname.indexOf("/tax") >= 0;
            }
            function isTaxGraph() {
              const queryParams = new URLSearchParams(location.search);
              const graphType = queryParams.get("graphType");
              return ["taxProfile", "taxIntake"].indexOf(graphType) >= 0;
            }
            function isTax() {
              return isTaxURL() || isTaxGraph();
            }


            function getCookie(name) {
              const value = "; " + document.cookie;
              const parts = value.split("; " + name + "=");
              if (parts.length >= 2) return parts.pop().split(";").shift();
            }

            function getGaSessions() {
              const cookie = {};
              document.cookie.split(";").forEach((el) => {
                const splitCookie = el.split("=");
                const key = splitCookie[0].trim();
                cookie[key] = splitCookie[1];
              });
              const sessions = Object.keys(cookie).filter((key) => key.startsWith("_ga"));
              const returnObj = {};
              sessions.forEach((session) => {
                let sessionSplit = session.split("_ga_");
                if (sessionSplit.length > 1) {
                  let sessionString = cookie[session];
                  returnObj["G-" + session.split("_ga_")[1]] = sessionString.split(".")[2];
                }
              });
              return returnObj;
            }

            function getGaClientId() {
              return getCookie("_ga")?.substring(6);
            }

            function getGoogleWbraid() {
              let wbraid = getCookie("_gcl_gb");
              if (wbraid && wbraid.split(".").length === 3) {
                return wbraid.split(".")[2];
              }
              return undefined;
            }

            function checkIfCookieValueExists(cookiename) {
              if (typeof cookiename == "string" && cookiename != "") {
                const COOKIES = document.cookie.split(";");
                for (i = 0; i < COOKIES.length; i++) {
                  if (COOKIES[i].trim().startsWith(cookiename)) {
                    return COOKIES[i].split("=")[1];
                  }
                }
              }
              return null;
            }
            function setGaClientId(context, clientId) {
              if (!context.integrations) {
                context.integration = {};
              }
              if (!context.integration["Google Analytics"]) {
                context.integration["Google Analytics"] = {};
              }
              context.integration["Google Analytics"].clientId = clientId;
            }

          function getUserContext() {
              const getIntercomDeviceId = () =>
                checkIfCookieValueExists(getCookie("intercom-device-id-cbw30hb8")) !== null
                  ? getCookie("intercom-device-id-cbw30hb8")
                  : getCookie("intercom-device-id-br2tge1x");
              const { country, state } = window.OneTrust.getGeolocationData();
              const isAgent = getCookie("LOGGEDIN_AS_AGENT") === "1";
              const isLoggedIn = getCookie("IsGuestUser")?.toLowerCase() === "false";
              return {
                google_click_id: getCookie("sem_gclid"),
                google_wbraid: getGoogleWbraid(),
                bazaar_voice_brand_id: getCookie("BVBRANDID"),
                quantum_metric_user_id: getCookie("QuantumMetricUserID"),
                launch_darkly_id: "",
                intercom_device_id: getIntercomDeviceId(),
                user_pseudo_id: getGaClientId(),
                optimizely_end_user_id: getCookie("optimizelyEndUserId"),
                lz_cid: getCookie("lzCID"),
                lz_uuid: getCookie("lz_uuid"),
                ga_sessions: getGaSessions(),
                fbp: getCookie("_fbp"),
                fbc: getCookie("_fbc"),
                // TikTok
                ttp: getCookie("_ttp"),
                ttclid: getCookie("ttclid"),
                msclkid: getCookie("_uetmsclkid"),
                //Reddit
                rdt_cid: getCookie('rdt_cid'),
                rdt_uuid: getCookie('_rdt_uuid'),
                // commission junction
                cj_event: getCookie('cjevent_tracking'),
                // trade desk id
                tdid: getCookie('ttd_TDID'),
                //geo data
                geo_country: country,
                geo_state: state,
                is_agent: isAgent,
                is_logged_in: isLoggedIn,
                is_internal_user: false, // TODO: evaluate for internal user
                is_tax: isTax()
              };
            }

            function checkAndParse(data) {
              if (!data) {
                return;
              }
              try {
                return JSON.parse(data);
              } catch(e) {}
            }


          function getDataAttribute(target, attribute, fromChild) {
              let parentOrChild;
              if (fromChild) {
                parentOrChild = target.querySelector("["+attribute+"]");
              } else {
                parentOrChild = target.closest("["+attribute+"]");
              }
              if (!parentOrChild) {
                return;
              }
              const attributeString = parentOrChild.getAttribute(attribute);
              return checkAndParse(attributeString);
            }

          function trackClicks() {
              document.addEventListener(
                "pointerdown",
                function (e) {
                  let { target } = e || {};
                  if (!target || !target.closest) {
                    return;
                  }
                  target =
                    target.closest("button") ||
                    target.closest("[role=button]") ||
                    target.closest("a") ||
                    target.closest("[role=link]") ||
                    target.closest("[role=menuitem]");
                  if (!target) {
                    return;
                  }
                  let { id="", tagName = "", textContent = "", href = "", ariaLabel = "" } = target || {};
                  textContent = textContent.replace(/[\\r\\n ]+/g, " ");
                  const tagNameLowerCase = tagName.toLowerCase();
                  const ctaCopy = textContent;
                  const identifier = target.getAttribute("data-tracking-label") || ariaLabel || textContent || id || "svg";
                  const interactionName =
                    tagNameLowerCase + " | " + "click" + " | " + identifier || "";
                  const trackType = tagNameLowerCase === "a" ? "link" : tagNameLowerCase;
                  const pageSection = findPageSection(target);
                  const overrides = getDataAttribute(target, "data-tracking-overrides");
                  const additionals = getDataAttribute(target, "data-tracking-additionals");
                  let properties = {
                    interaction_name: interactionName,
                    page_section: pageSection,
                    track_type: trackType,
                    cta_copy: ctaCopy,
                    target: href,
                    html_element_id: target.id,
                  };
                  properties = Object.assign({}, properties, overrides, {additional_details: additionals});
                  window.analytics.track(
                    "Interaction",
                    properties,
                    {}
                  );
                },
                true
              ); // true here handles the event in capture phase
            }

          function trackInput() {
              document.addEventListener(
                "change",
                function (e) {
                  if (!e || !e.target) {
                    return;
                  }
                  const {
                    id = "",
                    value = "",
                    name = "",
                    tagName = "",
                    type = "",
                    ariaLabel = "",
                  } = e.target;
                  // DO NOT TRACK USER INPUT BY DEFAULT
                  let userInput = "NOT_TRACKED";
                  let trackValue = false;
                  if (e.target.getAttribute("data-track-input") === "true") {
                    trackValue = true;
                  }
                  if (trackValue) {
                    userInput = value; // need to know which all apps need this user_input value to be tracked
                  }
                  if (appName === "margo") {
                    userInput = value; // need to know which all apps need this user_input value to be tracked
                  }
                  // DO NOT TRACK TAX INPUT
                  if (isTax()) {
                    userInput = "NOT_TRACKED";
                  }
                  const tagNameLowerCase = tagName?.toLowerCase();
                  let textContent = e.target.getAttribute("data-tracking-label") || name || ariaLabel || id;
                  const interactionName =
                    tagNameLowerCase + " | " + "change" + " | " + textContent || "";
                  const pageSection = findPageSection(e.target);
                  const overrides = getDataAttribute(e.target, "data-tracking-overrides");
                  const additionals = getDataAttribute(e.target, "data-tracking-additionals");
                  let properties = {
                    interaction_name: interactionName,
                    page_section: pageSection,
                    track_type: tagNameLowerCase,
                    target: textContent,
                    user_input: userInput,
                    html_element_id: id,
                  };
                  properties = Object.assign({}, properties, overrides, {additional_details: additionals});
                  window.analytics.track(
                    "Interaction",
                    properties,
                    {}
                  );
                },
                true
              ); // true here handles the event in capture phase
            }

    function setupAnalyticsMiddlewares() {
      // Register OneTrust Integration plugin
      //NOTE: registered plugins are executed after middlewares
      // context.consent was not available during middlware execution
        const setOneTrustContext = (ctx) => {
          if (ctx.event.context.consent) {
            ctx.event.context.consent.google = {
              ad_storage: ctx.event.context.consent.categoryPreferences["C0004"]
                ? "granted"
                : "denied",
              analytics_storage: ctx.event.context.consent.categoryPreferences[
                "C0005"
                ]
                ? "granted"
                : "denied",
            };
          }
          return ctx;
        };
        window.analytics?.register({
          name: "OneTrust Integration Cookie",
          version: "0.1.0",
          type: "enrichment",
          isLoaded: () => true,
          load: () => Promise.resolve(),
          // context object and reference to the analytics.js instance
          page: setOneTrustContext,
          track: setOneTrustContext,
          identify: setOneTrustContext,
        });

      // bot identification
      function botFilterMiddleware(middlewareContext) {
        var excludedUserAgents = [
          'Googlebot',
          'Bingbot',
          'Slurp',
          'DuckDuckBot',
          'Baiduspider',
          'Datadog',
          'Cypress',
          'YandexBot',
          'bingbot',
          'Impact Radius Compliance Bot',
          'adsbot',
          'yandexmobilebot',
          'applebot',
          'petalbot',
          'VirusTotalBot',
        ];

        const payload = middlewareContext.payload;
        const userAgent = navigator.userAgent;
        const isBot = excludedUserAgents.some((bot) => userAgent.includes(bot));
        if (isBot) {
          return null;
        }
        middlewareContext.next(payload);
      }

      function contextUpdateMiddleware(middlewareContext) {
        // setting context
        const payload = middlewareContext.payload;
        const { search } = payload.obj.context.page;
        if (search.length) {
          payload.obj.context.page.searchParams = Object.fromEntries(new URLSearchParams(search));
        }
        if (payload.obj.context) {
          payload.obj.context.identifiers = getUserContext();
          payload.obj.context.internal_event_id = window.generateEventID(
            payload.obj.name || payload.obj.event,
            'segment',
          );
          setGaClientId(payload.obj.context, getGaClientId());
          if (localStorage.getItem("ajs_user_traits")) {
            try {
              payload.obj.context.traits = JSON.parse(
                localStorage.getItem("ajs_user_traits")
              );
            } catch (e) {}
          }
        }
        middlewareContext.next(payload);
      }


      function propertyUpdateMiddleware(middlewareContext) {
        // setting properties
        const payload = middlewareContext.payload;
        const screenName = window.location.pathname;
        const defaultProperties = {
          hash: window.location.hash,
          category: "drupal",
          screen_name: screenName,
        };
        if (payload.obj.properties) {
          payload.obj.properties = Object.assign(
            {},
            defaultProperties,
            payload.obj.properties
          );
        }
        middlewareContext.next(payload);
      }

      window.analytics.addSourceMiddleware(botFilterMiddleware);
      window.analytics.addSourceMiddleware(contextUpdateMiddleware);
      window.analytics.addSourceMiddleware(propertyUpdateMiddleware);

      window.analytics.page("Page Viewed", {});
    }
      window.analytics.ready(() => {
        setupAnalyticsMiddlewares();
      });
       window.addEventListener("load", (event) => {
              trackClicks();
              trackInput();
            });
      withOneTrust(analytics).load("TiERpYXvvRVle5SyXPSNJssBISaF5Yyd", { integrations: { "Segment.io": { apiHost: "proxy.legalzoom.com/segments-io/v1" }}});
    }}();
  </script>
